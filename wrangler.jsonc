{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "pansou",
  "main": "worker.js",
  "compatibility_date": "2025-05-05",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_als"
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "./build"
  },
  "d1_databases": [
    {
      "binding": "MAINDB",
      "database_name": "maindb",
      "database_id": "52bec4a2-a12a-484d-8f58-4f254b8cffd0"
    },
    /**
    {
      "binding": "PANSOUDB",
      "database_name": "pansou2",
      "database_id": "4e48fcd0-a8fa-4dca-a8f9-021184e57753"
    },
    {
      "binding": "PANSOUDB",
      "database_name": "pansou3",
      "database_id": "b6e33f0e-061e-4ff9-8ac6-6f80f86b7d4d"
    },
    {
      "binding": "PANSOUDB",
      "database_name": "pansou4",
      "database_id": "0bce0745-a204-4382-b16f-c03e827a33f2"
    },
    {
      "binding": "PANSOUDB",
      "database_name": "pansou5",
      "database_id": "cd5d8762-0272-451c-b0d4-f4881016b6ad"
    },
    {
      "binding": "PANSOUDB",
